  `device_name` varchar(150) DEFAULT NULL,
  `last_activity` datetime NOT NULL,
  PRIMARY KEY (`device_unique_id`),
  UNIQUE KEY `UNQ_MOB_DEV_UNQ` (`device_unique`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=latin1;
